    "lint-fix": "tslint --fix src/**/*.ts -t verbose",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/masonz/v-hover.git"
  },
