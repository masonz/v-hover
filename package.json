  "dependencies": {
    "tslint-config-airbnb": "^5.3.0"
  },
  "devDependencies": {
    "@types/node": "^8.0.28",
    "autoprefixer": "^7.1.4",
    "awesome-typescript-loader": "^3.2.3",
    "cross-env": "^5.0.5",
    "css-loader": "^0.28.7",
    "extract-text-webpack-plugin": "^3.0.0",
    "node-sass": "^4.5.3",
